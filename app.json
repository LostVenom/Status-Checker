{
    "name": "Lost Status Checker",
    "description": "Best Telegram Bot for Updating Bot Alive satus.",
    "keywords": [
    "Lost Status Bot",
    "Telegram",
    "Alive",
    "Status"
  ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "SESSION_STRING": {
            "description": "A valid Pyrogram session string",
            "required": true
        },
        "BOT_LIST": {
            "description": "Your bot username list without '@' (Example: HvCodex T_Series)",
            "required": true
        },
       "CHANNEL_ID": {
            "description": "Your channel's or group's Telegram id (Example: -1001982130535)",
            "required": true
        },
       "GRP_ID":{
            "description": "Your Group Id To Send Warnings.",
            "required": true
       },
       "MESSAGE_ID": {
            "description": "Telegram id of message from your channel or group (Example: 42)",
            "required": true
        },
        "BOT_ADMIN_IDS": {
            "description": "Bot admin(s) id(s) (Example: 1215104805)"
        },
        "TIME_ZONE": {
            "description": "Your time zone (Example: Asia/Kolkata)",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "eco"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
