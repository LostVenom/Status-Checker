{
    "name": "Lost Status",
    "description": "Telegram Bot Checker for checking bot status.",
    "repository": "https://github.com/LostVenom/Status-Checker",
    "keywords": [
        "BotStatus",
        "live",
        "Up or down",
        "Check status of bots",
        "Telegram userbot",
        "Pyrogram userbot"
    ],
    "env": {
        "API_ID": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "Get from my.telegram.org/apps",
            "required": true
        },
        "SESSION_STRING": {
            "description": "A Pyrogram session string",
            "required": true
        },
        "BOT_LIST": {
            "description": "Your bot username list without '@' (Example: LostBot HVCodexBot)",
            "required": true
        },
       "CHANNEL_ID": {
            "description": "Your channel's or group's Telegram id (Example: -1002124303816)",
            "required": true
        },
       "GRP_ID":{
            "description": "Your Group Id To Send Warnings.",
            "required": true
       },
       "MESSAGE_ID": {
            "description": "Telegram id of message from your channel or group (Example: 10)",
            "required": true
        },
        "BOT_ADMIN_IDS": {
            "description": "Bot admin(s) id(s) (Example: 1298476502 2976398265)"
        },
        "TIME_ZONE": {
            "description": "Your time zone (Example: Asia/Kolkata)",
            "required": true
        }
    },
    "formation": {
    "worker": {
      "quantity": 1,
      "size": "eco"
    }
  },
    "buildpacks": [
        {
            "url": "heroku/python"
        }
    ]
}
